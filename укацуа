    <script>
        function openPDF(element) {
          const pdfUrl = element.getAttribute('data-pdf');
          if (pdfUrl) {
            window.location.href = pdfUrl; // Или window.open(pdfUrl, '_blank'); для новой вкладки
          } else {
            console.error('URL PDF не найден в атрибуте data-pdf.');
          }
        }
        </script>
        
    
